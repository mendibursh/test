import primitives.*;
public class Camera  {
	
	Point3D _p0;
	Vector _vUp;
	Vector _vTo;
	Vector _vRight;
	
	public Camera(Point3D _p0, Vector _vUp, Vector _vTo) {
		
		if(_vUp == null || _vTo == null) 
		this._p0 = _p0;
		this._vUp = new Vector(_vUp);
		this._vTo = new Vector(_vTo);
		this._vRight = (_vTo.CrossProduct(_vTo, _vUp)) ;
	}
	
}
